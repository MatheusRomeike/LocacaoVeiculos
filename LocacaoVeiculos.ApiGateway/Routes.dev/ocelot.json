{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "SwaggerKey": "AUTH",
      "UseServiceDiscovery": true,
      "ServiceName": "LocacaoVeiculos.AuthService",
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/api/payment/{everything}",
      "DownstreamScheme": "http",
      "SwaggerKey": "PAYMENT",
      "UseServiceDiscovery": true,
      "ServiceName": "LocacaoVeiculos.PaymentService",
      "UpstreamPathTemplate": "/api/payment/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/api/rental/{everything}",
      "DownstreamScheme": "http",
      "SwaggerKey": "RENTAL",
      "UseServiceDiscovery": true,
      "ServiceName": "LocacaoVeiculos.RentalService",
      "UpstreamPathTemplate": "/api/rental/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    },
    {
      "DownstreamPathTemplate": "/api/vehicle/{everything}",
      "DownstreamScheme": "http",
      "SwaggerKey": "VEHICLE",
      "UseServiceDiscovery": true,
      "ServiceName": "LocacaoVeiculos.VehicleService",
      "UpstreamPathTemplate": "/api/vehicle/{everything}",
      "UpstreamHttpMethod": ["GET", "POST", "PUT", "DELETE"],
      "LoadBalancerOptions": {
        "Type": "RoundRobin"
      }
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "DownstreamScheme": "http",
    "UseServiceDiscovery": true,
    "ServiceDiscoveryProvider": {
      "Port": 80,
      "Type": "Eureka",
      "Host": "apigateway"
    }
  },
  "SwaggerEndPoints": [
    {
      "Key": "apigateway",
      "TransformByOcelotConfig": false,
      "Config": [
        {
          "Name": "apigateway",
          "Version": "1.0",
          "Url": "http://apigateway:80/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "AUTH",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Auth.API",
          "Version": "1.0",
          "Url": "http://apigateway:80/auth/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "PAYMENT",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Payment.API",
          "Version": "1.0",
          "Url": "http://apigateway:80/payment/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "RENTAL",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Rental.API",
          "Version": "1.0",
          "Url": "http://apigateway:80/rental/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "VEHICLE",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Vehicle.API",
          "Version": "1.0",
          "Url": "http://apigateway:80/vehicle/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
